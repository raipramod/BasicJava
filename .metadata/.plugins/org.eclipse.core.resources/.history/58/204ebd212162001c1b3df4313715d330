
public class Multithread extends Thread {

	public static void main(String[] args) {

		Multithread mt = new Multithread();
		mt.start();
		
	for(int q = 0; q<=10; q++)	{
		System.out.print("q: " +q + "\t");
	}
	}
	
	public void run() {
		for(int p = 1;p<=10; p++) {
		System.out.print("p : " + p + "\t");	
		}
	}

}
